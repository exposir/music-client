{
    "name": "qianqian-music",
    "version": "12.0.3",
    "author": "北京音之邦文化科技有限公司",
    "description": "千千音乐",
    "main": "./dist/electron/main.js",
    "scripts": {
        "packer": "node tasks/package.js",
        "build": "node .electron-vue/build.js && electron-builder",
        "buildjs": "node .electron-vue/build.js",
        "buildInstall": "electron-builder",
        "build:dir": "node .electron-vue/build.js && electron-builder --dir",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
        "dev": "node .electron-vue/dev-runner.js",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
        "postinstall": ""
    },
    "build": {
        "productName": "QianqianMusic",
        "afterPack": "tasks/afterPackHook.js",
        "appId": "com.baidu.music",
        "directories": {
            "output": "build"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "build/icons/icon.ico",
            "uninstallerIcon": "build/icons/icon.ico",
            "deleteAppDataOnUninstall": true,
            "createDesktopShortcut": true,
            "runAfterFinish": true,
            "createStartMenuShortcut": true,
            "shortcutName": "千千音乐尝鲜版",
            "license": "build/Licence.txt"
        },
        "files": [
            "dist/electron/**/*"
        ],
        "dmg": {
            "background": "build/background.png",
            "contents": [{
                    "x": 580,
                    "y": 230,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 200,
                    "y": 230,
                    "type": "file"
                }
            ],
            "iconSize": 100,
            "window": {
                "width": 780,
                "height": 460
            }
        },
        "extraResources": "zh-Hans.lproj/*",
        "mac": {
            "category": "public.app-category.music",
            "icon": "build/icons/icon.icns",
            "type": "distribution",
            "provisioningProfile": "BaiduMusicMac.provisionprofile",
            "helperBundleId": "com.baidu.music",
            "extendInfo": {
                "CFBundleDevelopmentRegion": "zh-CN",
                "LSHasLocalizedDisplayName": true,
                "ElectronTeamID": "BCP9BVS9YN"
            },
            "target": [
                "dmg",
                "mas"
            ]
        },
        "win": {
            "icon": "build/icons/icon.ico",
            "target": {
                "target": "nsis",
                "arch": [
                    "ia32"
                ]
            },
            "extraFiles": [{
                    "from": "src/renderer/audiocore",
                    "to": "./",
                    "filter": [
                        "**/*",
                        "!src/renderer/audiocore/*.js"
                    ]
                },
                {
                    "from": "dist/config",
                    "to": "./"
                }
            ]
        }
    },
    "dependencies": {
        "area-data": "^5.0.6",
        "crypto-js": "^3.1.9-1",
        "element-ui": "1.3.7",
        "getmac": "^1.2.1",
        "md5": "^2.2.1",
        "qrcodejs2": "0.0.2",
        "threads": "^0.12.0",
        "underscore": "^1.8.3",
        "velocity-animate": "^1.5.0",
        "vue": "^2.5.13",
        "vue-area-linkage": "^5.0.0",
        "vue-awesome-swiper": "^3.1.3",
        "vue-lazyload": "^1.1.4",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1",
        "vuex-router-sync": "^5.0.0",
        "zeltice-mt-downloader": "^1.1.3"
    },
    "devDependencies": {
        "archiver": "^2.1.1",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-minify-webpack-plugin": "^0.3.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "cfonts": "^1.1.3",
        "chalk": "^2.1.0",
        "chokidar": "^2.0.4",
        "copy-webpack-plugin": "^4.0.1",
        "cropperjs": "^1.4.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.4",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^4.0.0",
        "electron-builder": "^20.38.4",
        "electron-debug": "^1.5.0",
        "electron-devtools-installer": "^2.2.0",
        "electron-log": "^2.2.17",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "multispinner": "^0.2.1",
        "native-ext-loader": "^2.1.0",
        "node-loader": "^0.6.0",
        "style-loader": "^0.18.2",
        "tree-kill": "^1.1.0",
        "url-loader": "^0.5.9",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^13.0.5",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.4.2",
        "webpack": "^3.5.2",
        "webpack-dev-server": "^2.7.1",
        "webpack-hot-middleware": "^2.18.2",
        "winreg": "^1.2.4"
    }
}